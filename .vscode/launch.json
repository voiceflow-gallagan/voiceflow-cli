{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Version command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
